<?php

namespace GemeenteAmsterdam\FixxxSchuldhulp\Allegro\Login\Type;

class TJaNeeVraag
{

    /**
     * @var string
     */
    private $Vraag;

    /**
     * @var bool
     */
    private $Antwoord;

    /**
     * @return string
     */
    public function getVraag()
    {
        return $this->Vraag;
    }

    /**
     * @param string $Vraag
     * @return TJaNeeVraag
     */
    public function withVraag($Vraag)
    {
        $new = clone $this;
        $new->Vraag = $Vraag;

        return $new;
    }

    /**
     * @return bool
     */
    public function getAntwoord()
    {
        return $this->Antwoord;
    }

    /**
     * @param bool $Antwoord
     * @return TJaNeeVraag
     */
    public function withAntwoord($Antwoord)
    {
        $new = clone $this;
        $new->Antwoord = $Antwoord;

        return $new;
    }


}

