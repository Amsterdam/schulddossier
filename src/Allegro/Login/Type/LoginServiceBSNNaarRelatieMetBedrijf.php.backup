<?php

namespace GemeenteAmsterdam\FixxxSchuldhulp\Allegro\Login\Type;

class LoginServiceBSNNaarRelatieMetBedrijf
{

    /**
     * @var int
     */
    private $BSN;

    /**
     * @return int
     */
    public function getBSN()
    {
        return $this->BSN;
    }

    /**
     * @param int $BSN
     * @return LoginServiceBSNNaarRelatieMetBedrijf
     */
    public function withBSN($BSN)
    {
        $new = clone $this;
        $new->BSN = $BSN;

        return $new;
    }


}

